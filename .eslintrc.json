{
  "env": {
    "jest": true,
    "browser": true
  },
  "extends": [
    "xo-space",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": true,
        "bracketSpacing": true
      }
    ]
  }
}
